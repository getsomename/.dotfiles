{
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.lineNumbers": "on",
    "window.zoomLevel": 0,
    "window.restoreWindows": "none",
    "editor.suggestOnTriggerCharacters": false,
    "editor.quickSuggestionsDelay": 10,
    // "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.highlightActiveIndentGuide": false,
    "debug.internalConsoleOptions": "neverOpen",
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    // "workbench.colorCustomizations": {
    //     // "editorError.border": "#ff000049"
    //     "editorInfo.foreground": "#ff000000",
    //     "editorError.foreground": "#ff000000"
    // },
    "window.openFilesInNewWindow": "on",
    "workbench.editor.showTabs": false,
    "workbench.activityBar.visible": false,
    "dart.flutterSdkPath": "/Users/getsomename/development/flutter",
    "dart.promptToGetPackages": true,
    "dart.runPubGetOnPubspecChanges": false,
    "dart.updateImportsOnRename": true,
    "git.autofetch": true,
    "zenMode.centerLayout": false,
    "zenMode.hideStatusBar": false,
    "editor.minimap.enabled": false,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "breadcrumbs.enabled": true,
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "explorer.openEditors.visible": 0,
    "workbench.colorTheme": "Visual Studio Light",
    "workbench.iconTheme": "awesome-icons",
    "git.enableSmartCommit": true,
    "dance.normalMode.lineNumbers": "off",
    "clojureVSCode.autoStartNRepl": false,
    // "vim.useCtrlKeys": false,
    "vim.statusBarColorControl": false,
    "vim.easymotion": false,
